            volScalarFieldValue alpha.particles 0
        );
    }
    zoneToCell
    {
        name "wall0";
        fieldValues
        (
            volScalarFieldValue alpha.air 1
            volScalarFieldValue alpha.particles 0
        );
    }
    zoneToCell
    {
        name "wall1";
        fieldValues
        (
            volScalarFieldValue alpha.air 1
            volScalarFieldValue alpha.particles 0
        );
    }
    zoneToCell
    {
        name "ring0";
        fieldValues
        (
            volScalarFieldValue alpha.air 1
            volScalarFieldValue alpha.particles 0
        );
    }
    zoneToCell
    {
        name "ring1";
        fieldValues
        (
            volScalarFieldValue alpha.air 1
            volScalarFieldValue alpha.particles 0
        );
    }
